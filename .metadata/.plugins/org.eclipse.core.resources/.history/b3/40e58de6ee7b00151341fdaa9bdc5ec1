import java.util.ArrayList;

public class Path {
	
	private ArrayList<Move> moves;
	public int length;
	
	public Path(int maxSteps){
		moves = new ArrayList<Move>();	
	}
	
	public Move[] getPath(){
		return (Move[]) moves.toArray();
	}
	
	public void addMove(Move move) {
		moves.add(move);
		this.length = moves.size();
	}
}
